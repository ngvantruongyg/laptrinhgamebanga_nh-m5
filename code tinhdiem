@Override
    public void keyTyped(KeyEvent e) {}

    private void checkCollisions() {
        bullets.removeIf(bullet -> {
            for (Chicken chicken : chickens) {
                if (bullet.getBounds().intersects(chicken.getBounds())) {
                    chickens.remove(chicken);
                    score += 20;
                    return true;
                }
            }
            return false;
        });

        for (Chicken chicken : chickens) {
            if (chicken.getBounds().intersects(player.getBounds())) {
                isGameOver = true;
                break;
            }
        }
    }
